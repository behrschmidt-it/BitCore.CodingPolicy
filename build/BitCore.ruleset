    <Rule Id="IDISP013" Action="Info" /> <!-- Await in Using -->
    <Rule Id="IDISP014" Action="Info" /> <!-- Use a single instance of HttpClient: Could be improved in the future -> HttpClientFactory -->
  </Rules>

