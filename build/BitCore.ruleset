  <Rules AnalyzerId="Microsoft.CodeQuality.Analyzers" RuleNamespace="Microsoft.CodeQuality.Analyzers">
    <!-- CA1024 Could conflict with S2365 - Propreties should not copy collections. Tells you Properties that copy collections should be a method GetX(). -->
    <Rule Id="CA1024" Action="Info" /> <!-- Use Properties when appropriate. Tells you methods with GetX() should be a property -->
    <Rule Id="CA1040" Action="None" /> <!-- Avoid Empty interfaces -->
    <Rule Id="CA1062" Action="Info" /> <!-- Validate arguments of public methods (Can not detect guard handling) -->
    <Rule Id="CA1724" Action="Info" /> <!-- Type names should not match namespaces  -->
    <Rule Id="CA1031" Action="None" /> <!-- Do not catch general exception types  -->
    <Rule Id="CA1034" Action="None" /> <!-- Nested types should not be visible  -->
    <Rule Id="CA1054" Action="None" /> <!-- Uri parameters should not be strings -->
    <Rule Id="CA1056" Action="None" /> <!-- Uri properties should not be strings  -->
    <Rule Id="CA1707" Action="None" /> <!-- Identifiers should not contain underscores -->
    <Rule Id="CA1710" Action="None" /> <!-- By convention, the names of types that extend certain base types or that implement certain interfaces, or types that are derived from these types, have a suffix that is associated with the base type or interface. -->
    <Rule Id="CA1714" Action="None" /> <!-- Flags enums should have plural names -->
    <Rule Id="CA1720" Action="None" /> <!-- Identifier contains type name -->
    <Rule Id="CA1802" Action="Info" /> <!-- Use literals where appropriate (readonly -> const) -->
    <Rule Id="CA1819" Action="Info" /> <!-- Properties should not return arrays,  IReadOnlyCollection is usally better, enable later as warning? -->
    <Rule Id="CA1822" Action="Warning" /> <!-- Mark members as static -->
    <Rule Id="CA2227" Action="Info" /> <!-- Collection properties should be read only (Should this be a warning?) -->
  </Rules>

  <Rules AnalyzerId="Microsoft.NetCore.Analyzers" RuleNamespace="Microsoft.NetCore.Analyzers">
    <Rule Id="CA1303" Action="None" /> <!-- Do not pass literals as localized parameters -->
    <Rule Id="CA1305" Action="Info" /> <!-- Specify IFormatProvider -->
    <Rule Id="CA1307" Action="Warning" /> <!-- Specify StringComparison -->
    <Rule Id="CA1308" Action="None" /> <!-- Normalize strings to uppercase -->
    <Rule Id="CA2000" Action="Info" /> <!-- Dispose objects before losing scope (Too many false positives) -->
    <Rule Id="CA2008" Action="None" /> <!-- Do not create tasks without passing a TaskScheduler -->
  </Rules>

  <Rules AnalyzerId="Microsoft.NetCore.CSharp.Analyzers" RuleNamespace="Microsoft.NetCore.CSharp.Analyzers">
    <Rule Id="CA1309" Action="Warning" /> <!-- Use ordinal StringComparison -->
  </Rules>

  <Rules AnalyzerId="Microsoft.CodeQuality.CSharp.Analyzers" RuleNamespace="Microsoft.CodeQuality.CSharp.Analyzers">
    <Rule Id="CA1032" Action="None" /> <!-- Implement standard exception constructors-->
    <Rule Id="CA2234" Action="Info" /> <!-- Pass system uri objects instead of strings -->
  </Rules>

  <Rules AnalyzerId="IDisposableAnalyzers" RuleNamespace="IDisposableAnalyzers">
    <Rule Id="IDISP001" Action="Info" /> <!-- Dispose created.: Who created disposable should dispose it  -->
